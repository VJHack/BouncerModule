<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_map"
        android:name="com.example.bouncermodule.ui.dashboard.DashboardFragment"
        android:label="@string/map"
        tools:layout="@layout/fragment_map" />
    <fragment
        android:id="@+id/navigation_bars"
        android:name="com.example.bouncermodule.ui.bars.BarsFragment"
        android:label="Bars"
        tools:layout="@layout/fragment_bars" />
    <fragment
        android:id="@+id/navigation_home"
        android:name="com.example.bouncermodule.ui.home.HomeFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_home" />


</navigation>